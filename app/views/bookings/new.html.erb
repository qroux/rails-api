<p>Je rend mon appartement disponible pour la dur√©e suivante</p>

<%= simple_form_for([@developer, @booking]) do |f| %>
  <%= f.label "appartement" %>
  <%= f.input :listing_id, collection: @listings %>
  <%= f.label "from" %>
  <%= f.date_field :start_date, min: Date.today, label: "From" %>
  <%= f.label "to" %>
  <%= f.date_field :end_date, min: Date.today, label: "From" %>
  <%= f.submit "book"%>
<% end %>
